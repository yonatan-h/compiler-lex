{ sample program in MINI language – computes factorials }

read x; { input an integer }
if x > 0 then { don’t compute if x<= 0}
    fact:= 1;
    repeat
        fact := fact * x ;
        x:= x-1
    until x = 0;

    write fact; { output factorial of x}
else
    fact := -1;
    write fact; {output -1 to show no solution}  
end

if 5 > 2
    write 1*(1+1);
end

if 5 = 5
    write 1+1;
end 

if 2 < 5
    write 1/1;
end